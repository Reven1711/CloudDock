# Files Service Configuration
NODE_ENV=development
FILE_SERVICE_PORT=4004

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster0.mongodb.net/CloudDock?retryWrites=true&w=majority
MONGODB_DB_NAME=CloudDock

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-clouddock-bucket

# Virus Scanning Configuration (Optional)
ENABLE_VIRUS_SCAN=false
VIRUS_SCAN_LAMBDA=clouddock-virus-scanner

# CORS Configuration (optional, handled by gateway)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

